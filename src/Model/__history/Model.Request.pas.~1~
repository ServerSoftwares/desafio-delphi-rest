unit Model.Request;

interface

uses
  Model.Request.Interfaces;

type
  TRequest = class(TInterfacedObject, IRequest)
  private

  public
    class function New: IRequest;

    function CEP: IRequestCEP;
  end;

implementation

uses
  Model.Request.Factory;

{ TRequest }

class function TRequest.New: IRequest;
begin
  Result := Self.Create;
end;

function TRequest.CEP: IRequestCEP;
begin
  Result := TFactorytRequest.NewCEP;
end;

end.
